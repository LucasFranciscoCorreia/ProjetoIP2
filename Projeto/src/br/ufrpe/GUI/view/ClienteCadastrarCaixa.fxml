            <Font size="14.0" />
         </font>
      </TextField>
      <Button contentDisplay="TOP" layoutX="545.0" layoutY="362.0" mnemonicParsing="false" onAction="#cadastrarCliente" text="Salvar">
         <graphic>
            <ImageView fitHeight="48.0" fitWidth="46.0">
               <image>
                  <Image url="@../../../../../lib/Imgs/save-file.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <Label fx:id="avisoCadastro" alignment="CENTER" layoutX="15.0" layoutY="310.0" prefHeight="41.0" prefWidth="588.0" textFill="RED">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <VBox layoutX="656.0" prefHeight="602.0" prefWidth="144.0">
         <children>
            <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#voltarMenuClienteCaixa" prefHeight="213.0" prefWidth="181.0" text="Voltar" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="100.0" fitWidth="100.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/clienteMenu.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#abrirClienteAtualizarCaixa" prefHeight="200.0" prefWidth="200.0" text="Atualizar" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="100.0" fitWidth="100.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/mudar.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#abrirClientePesquisarCaixa" prefHeight="200.0" prefWidth="200.0" text="Pesquisar" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="100.0" fitWidth="100.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/busca.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </VBox>
