               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="75.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/add-128.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#abrirClientePesquisar" prefHeight="150.0" prefWidth="200.0" text="Pesquisar" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="75.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/busca.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </VBox>
      <Label layoutX="14.0" layoutY="14.0" prefHeight="27.0" prefWidth="181.0" text="CPF do cliente">
