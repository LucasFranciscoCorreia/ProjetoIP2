<AnchorPane fx:id="AvisoDelecao" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.ufrpe.GUI.model.ClienteController">
