               <font>
                  <Font size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="75.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/clienteMenu.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#abrirClienteAtualizar" prefHeight="150.0" prefWidth="200.0" text="Atualizar" textAlignment="RIGHT">
               <font>
                  <Font size="18.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="75.0" fitWidth="75.0">
                     <image>
                        <Image url="@../../../../../lib/Imgs/mudar.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Button fx:id="btnCadastrar" alignment="CENTER" contentDisplay="TOP" mnemonicParsing="false" onAction="#abrirClienteCadastrar" prefHeight="150.0" prefWidth="200.0" text="Cadastrar" textAlignment="RIGHT">
