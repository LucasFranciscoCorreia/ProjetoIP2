            <Font size="14.0" />
         </font>
      </TextField>
      <Button fx:id="buttonProcurar" layoutX="14.0" layoutY="82.0" mnemonicParsing="false" onAction="#buscarCliente" prefHeight="30.0" prefWidth="103.0" text="Pesquisar">
