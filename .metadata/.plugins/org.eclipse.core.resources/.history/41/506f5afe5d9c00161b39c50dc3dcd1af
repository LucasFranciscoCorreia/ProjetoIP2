package br.ufrpe.beans;
public class Login {
	private String login;
	private int senha;
	
	public Login(String login, int senha) {
		this.login = login;
		this.senha = senha;
	}
	public String getLogin() {
		return login;
	}
	public void setLogin(String login) {
		this.login = login;
	}
	public int getSenha() {
		return senha;
	}
	public void setSenha(int senha) {
		this.senha = senha;
	}
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + ((login == null) ? 0 : login.hashCode());
		result = prime * result + senha;
		return result;
	}
	@Override
	public boolean equals(String login, int senha;) {
		if(this.login.equalsIgnoreCase(login) && this.senha == senha){
			return true;
		}else{
			return false;
		}
	}
	
	
}
